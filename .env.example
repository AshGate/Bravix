# Bot Configuration (REQUIS)
DISCORD_TOKEN=your_bot_token_here
DISCORD_CLIENT_ID=your_client_id_here
DISCORD_CLIENT_SECRET=your_client_secret_here

# Admin User ID (REQUIS - votre ID Discord personnel)
ADMIN_USER_ID=your_discord_user_id_here

# Web Dashboard
WEB_PORT=3001

# Variables d'environnement pour le client (Vite)
VITE_DISCORD_CLIENT_ID=your_client_id_here

# Configuration Optionnelle (le bot fonctionnera sans ces IDs)
# Si vous voulez configurer des salons/rôles spécifiques, décommentez et remplissez :
# TICKET_CATEGORY_ID=your_category_id_here
# LOGS_CHANNEL_ID=your_logs_channel_id_here
# TRANSCRIPT_CHANNEL_ID=your_transcript_channel_id_here
# SUPPORT_ROLE_ID=your_support_role_id_here